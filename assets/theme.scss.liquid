//------------------------ TOGGLE FOR MOBILE MENU ------------------------
#check {
  display: none;
}
#check:checked ~ .menu-mobile{
  transform: translateX(0%);
}

#icon {
  display: inline-block;
  cursor: pointer;
  z-index: 1;
  position: relative;
}

/* bar icon */
.bar1,
.bar2,
.bar3 {
  width: 25px;
  height: 2px;
  background-color: #333;
  margin: 4px 0;
  transition: 0.5s;
  border-radius: 60px;
}

/* Rotate first bar icon menu*/
.change .bar1 {
  -webkit-transform: rotate(-45deg) translate(-4px, 4px);
  transform: rotate(-45deg) translate(-4px, 4px);
  background-color: #fff;
}

/* Fade out the second bar icon menu*/
.change .bar2 {
  opacity: 0;
}

/* Rotate last bar icon menu*/
.change .bar3 {
  -webkit-transform: rotate(45deg) translate(-5px, -5px);
  transform: rotate(45deg) translate(-5px, -5px);
  background-color: #fff;
}

.menu-mobile__close {
  padding: 5px;
  position: absolute;
  top: -12px;
  right: 0px;
  font-size: 30px;
  i {
    font-style: initial;
  }
  .fa:before {
    content: '\00d7';
  }
}

