//
// Base styles
//

.btn {
  display: inline-block;
  text-align: center;
  vertical-align: middle;
  user-select: none;
  border: 1px solid transparent;
  padding: $btn-padding;
  font-family: $btn-font-style;
  font-weight: 700;
  font-size: $btn-font-size;
  line-height: $btn-line-height;
  text-transform: uppercase;
  letter-spacing: 2px;
  transition: all 0.3s ease;
  white-space: normal;

  // Share hover and focus styles
  &:hover {
    text-decoration: none;
  }

  &:focus,
  &.focus {
    outline: 0;
    box-shadow: none;
  }

  // Disabled comes first so active can properly restyle
  &.disabled,
  &:disabled {
    opacity: 50;
    background: #ccc;
    pointer-events:none;
  }

  // Opinionated: add "hand" cursor to non-disabled .btn elements
  &:not(:disabled):not(.disabled) {
    cursor: pointer;
  }

  &:not(:disabled):not(.disabled):active,
  &:not(:disabled):not(.disabled).active {
    background-image: none;

    &:focus {
    }
  }
}

// Future-proof disabling of clicks on `<a>` elements
a.btn.disabled,
fieldset:disabled a.btn {
  pointer-events: none;
}

//
// Button Primary
//
.btn-primary {
  background: $btn-primary-bg-color;
  border: 2px solid $btn-primary-border-color;
  color: $btn-primary-color;
  &:hover, &:focus {
    background:  $btn-primary-hover-bg-color;
    color: $btn-primary-hover-color;
    border: 2px solid $btn-primary-hover-border-color;
  }
  &:active {
    background: $btn-primary-bg-color;
    border: 2px solid $btn-primary-border-color;
    color: $btn-primary-color;
  }
}

.btn-light {
  border: 1px solid #fff;
  color: #fff;
}

.btn.inactive {
  background: #ccc;
  pointer-events: none;
}

//
// Button Secondary
//
.btn-secondary {
  background: $btn-secondary-bg-color;
  border: 2px solid $btn-secondary-border-color;
  color: $btn-secondary-color;
  &:hover {
    background: #fff;
    color: #000;
  }
}

//
// Button Secondary
//
.btn-border {
  background: $btn-border-bg-color;
  border: 2px solid $btn-border-border-color;
  color: $btn-border-color;
  &:hover {
    border-color: transparent;
    color: #fff;
    background: #000;
  }
}

.btn-full {
  width: 100%;
}

//
// Button Sizes
//
.btn-small {
  padding: $btn-padding / 1.5;
}

//
// Button Basket/Checkout
//
.btn-black {
  background: #000;
  border: 1px solid transparent;
  color: #fff;
}

.btn-upsell-cart {
  padding: $btn-padding / 2;
}

