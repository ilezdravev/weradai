// Range slider:
.range__area {
  margin: 0 auto;
  width: 700px;
  height: 150px;
  position: relative;
  display: flex;
  align-items: center;
  @include mobile {
    margin-top: 40px;
    display: block;
    width: 95%;
    height: auto;
    min-height: 100px;
  }
  @include media(medium) {
    width: 70%;
  }
}
.range__slider {
  height: 4px;
  background: #ccc;
  position: absolute;
  width: calc(100% - 150px * 2 - 40px);
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  @include mobile {
    margin-top: 30px;
    width: 100%;
    position: relative;
    top: auto;
    left: auto;
    transform: none;
  }
}
.range__labels {
  flex: 1;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.range__label {
  width: 160px;
  letter-spacing: 1px;
  @include mobile {
    width: 150px;
    font-size: 13px;
  }
  &.m-left {
    text-align: right;
    @include mobile {
      text-align: left;
    }
  }
  &.m-right {
    text-align: left;
    @include mobile {
      text-align: right;
    }
  }
}
.range__shutter {
  width: 80px;
  height: 80px;
  background-color: #fff;
  background-repeat: no-repeat;
  background-position: center center;
  background-size: auto 70%;
  border-radius: 50%;
  border: none;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  &:focus, &:active {
    outline: none;
  }
  @include mobile {
    width: 60px;
    height: 60px;
  }
}

