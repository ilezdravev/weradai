/*================ MODULES ================*/
/*================ Site Header ================*/
.site-logo {
  display: block;

  img {
    display: block;
    width: 100%;
  }
}

// Nav common styles
.primary-header-wrapper, .secondary-header-wrapper {
  background: #fff;
}
.secondary-header-wrapper {
  box-shadow: 0 5px 4px rgba(0,0,0,0.03);
}
.nav {
  display: flex;
}
.nav.m-vertical {
  display: block;
  .nav__items {
    flex-direction: column;
    align-items: flex-start;
  }
  .nav__link {
    padding: $gutter/2 0;
  }
}
.nav.m-flex {
  display: flex;
  justify-content: space-between;
}
.nav__items {
  display: flex;
  align-items: center;
  justify-content: space-between;
  margin: 0;
  padding: 0;
  list-style: none;
  &.m-vertical {
    flex-direction: column;
    align-items: flex-start;
    justify-content: flex-start;
  }
}

.nav__link {
  display: inline-block;
  padding: $gutter;
  font-size: inherit;
  color: inherit;

  &:hover {
    text-decoration: none;
  }
}
.nav__link.m-icon {
  padding: 10px;
  &.is-open {
    .icon-caret-down {
      transform: rotate(-180deg);
    }
  }
  .icon,
  i {
    display: inline-block;
    vertical-align: middle;
    font-size: 1em;
    transition: 0.25s;
  }
}


// Nav secondary desktop
.nav--secondary {
  display: block;
  border-top: 1px solid #ececec;
  .nav__items.m-level-1 {
    margin: 0 (-$gutter);
    justify-content: center;
    align-items: stretch;
  }
  .nav__item {
    margin: 0;
    &.m-level-1 {
      padding: 10px;
      @include media(x-large) {
        padding: 10px 19px;
      }
      &:hover {
        .nav__link.m-level-1 {
          border-bottom-color: currentColor;
        }
      }
      // &.is-active {
      //   .nav__link.m-level-1 {
      //     border-bottom-color: currentColor;
      //   }
      // }
    }
  }
  .nav__link.m-level-1 {
    padding: 0 0 0.15em;
    border-bottom: 1px solid transparent;
    white-space: nowrap;
    font-weight: 300;
  }
}


// Nav dropdown
.nav-dropdown {
  padding: $gutter*6 0 $gutter;
  display: flex;
  align-items: flex-start;
  position: absolute;
  top: 0%;
  left: 0;
  right: 0;
  background: #fff;
  pointer-events: none;
  transition: 0.33s all ease;
  z-index: -1;
  transform: translate(0, -105%);
  &.reduced-padding {
    padding: $gutter*5 0 $gutter;
  }
  .container {
    //display: flex;
    //align-items: stretch;
    .border-left {
      &:before {
        content: '';
        display: block;
        position: absolute;
        top: 0;
        left: 0;
        height: 2px;
        background: #ccc;
        height: 100%;
        width: 1px;
        margin-left: -13px;
      }
    }
  }
  .nav__item:hover & {
    pointer-events: auto;
    transform: translate(0, 0);
    .container {

    }
  }


  .nav__items.m-level-2 {
    margin-left: -15px;
    display: block;
  }

  .nav__link.m-level-2 {
    padding: 5px 15px;
    font-weight: 400;
    &:hover {
      text-decoration: underline;
    }
  }
}

.nav-dropdown__feature {
  flex: 1;
  padding-left: $gutter;
  max-width: 50%;
  margin-left: auto;
  border-left: 1px solid #ccc;
}
.nav-dropdown__title {
  margin-top: 0;
  font-weight: 300;
  text-transform: uppercase;
  margin-bottom: 10px;
  color: #333;
}

.image-link-row {
  margin: 0 (-$gutter/2);
  display: flex;
  align-items: flex-start;
}
.image-link-col {
  flex: 1;
  @include mobile {
    min-width: 50%;
    max-width: 50%;
  }
}
.image-link {
  display: block;
  &:hover {
    text-decoration: none;
  }
}
.image-link__image {
  background-position: center center;
  background-size: cover;
  background-repeat: no-repeat;
}
.image-link__title {
  margin-top: 10px;
  line-height: 1.5;
  font-size: 16px;
  color: #000;
  font-weight: 400;
  margin-bottom: 0;
  .fa {
    font-size: 11px;
  }
  svg {
    width: 11px;
    margin-left: 2px;
    padding-bottom: 3px;
  }
}
.image-link__text {
  margin-top: 0px;
  line-height: 1.5;
  font-size: 14px;
  color: #000;
}


.nav-dropdown__block {
  // flex: 1;
  // padding-right: $gutter;
  // max-width: 50%;
}
.text-block {
  width: 100%;
  height: 100%;
  padding: $gutter;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  text-align: center;
}
.text-block__icon {
  width: 80px;
  height: 80px;
  background-color: #fff;
  background-repeat: no-repeat;
  background-position: center center;
  background-size: auto 70%;
  border-radius: 50%;
}
.text-block__text {
  @include title-font-style;
  font-size: 20px;
  max-width: 350px;
  p {
    font-family: inherit;
    font-size: inherit;
    line-height: inherit;
    font-weight: inherit;
    text-align: left;
  }
}




.mobile-menu-wrapper {
  height: $navbar-height-mini;
}

.menu-mobile {
  padding: $gutter;
  position: fixed;
  top: 3%;
  left: 3%;
  right: 3%;
  bottom: 3%;
  transition: all .5s;
  background: $mobile-nav-bg;
  z-index: 999;
  overflow: hidden;
  box-shadow: 10px 0px 30px rgba(0, 0, 0, 0.25);
  transform: translateX(-120%);
  padding-bottom: 65px;
}
.nav-mobile {
  width: 100%;
  height: 100%;
  overflow: auto;
  .nav__list {
    padding: 0;
    margin: 0;
