    p {
      margin-top: 10px;
    }
  }
  .colors-wrapper {
    .label {
      display: none;
    }
  }
}

.quick-add__hover {
  padding: 0 10px 10px;
  position: absolute;
  bottom: 10px;
  left: 10px;
  right: 10px;
  transition: 0.33s opacity;
  opacity: 0;
  z-index: -1;
  display: flex;
  flex-wrap: wrap;
  background-color: #fff;
  .swatch {
    display: flex;
    flex-wrap: wrap;
    justify-content: center;
    margin-top: 0;
    margin-left: -10px;
    flex-grow: 1;

    +.swatch {
      min-width: 70%;
      max-width: 70%;
    }
    .size-dropdown,
    .length-dropdown {
      width: 100%;
      height: 30px;
      min-width: 0;
      padding: 8px 0;
      margin-left: 10px;
      margin-top: 10px;
      background-color: #fff;
      border: none;
      border-bottom: 1px solid #000;
      font-weight: 300;
      text-transform: uppercase;
      font-size: 11px;
      background-position: right;
