    }
  }

  .selector-wrapper {
    display: none;
  }
  .cd-popup, .size-guide-button {
    display: none;
  }
  .swatch {
    .size-dropdown,
    .length-dropdown {
      width: 100%;
      min-width: 0;
      margin-right: 0;
      font-size: 0.88rem;
      padding: 10px;
      border: none;
    }

    .header, &:after {
      display: none;
    }
    label {
      min-width: 24px !important;
      height: 24px !important;
      line-height: 22px;
      font-size: 11px;
