.highlights {
  margin: $gutter 0;
  @include media-query('medium-up') {
    .row {
      justify-content: center;
    }

    .col-md-4 {
      max-width: 140px;
    }
  }
  @include media(small) {
    margin: $gutter 0 0;
    .col-md-4 {
      align-items: center;
    }
  }
}
.highlights__icon {
  height: 50px;
  background-position: center center;
  background-repeat: no-repeat;
  background-size: contain;
  @include media(small) {
    height: 30px;
  }
}
.highlights__text {
  margin-top: 1em;
  text-align: center;
  @include media(small) {
    margin-top: 0;
  }
}
@include media(small) {
  .highlights {
    .highlights__icon {
      width: 50px;
    }
    .col-md-4 {
      display: flex;
      margin-bottom: 10px;
    }
  }
}

//------------------------ TABS  ------------------------
.responsive-tabs {
  margin: 30px 0 0;
  display: block;
  @include media(small) {
    margin: 15px 0 0;
  }

  .tab_container {
  	border-top: none;
  	width: 100%;
  	background: #fff;
    border-bottom: 1px solid #000;
    ul {
      margin: 0;
      padding: 0 20px 0px;
    }
  }

  .tab_content {
  	padding: 0 0 20px;
  	display: none;
    p:first-child {
      margin-top: 0px;
    }
  }

  .tab_list__content {
    width: 100%;
  }

  .tab_drawer_heading {
    display: block;
    padding: 5px 0;
    margin: 0;
    position: relative;
    @include header-font-style;
    line-height: 35px;
    font-weight: bold;
    border-top: 1px solid #000;
    cursor: pointer;
    -webkit-touch-callout: none;
    -webkit-user-select: none;
    -khtml-user-select: none;
    -moz-user-select: none;
    -ms-user-select: none;
    user-select: none;
    &:after {
      content: "→";
      position: absolute;
      right: 0;
      top: 50%;
      transform: translate(0, -50%);
    }
    &.d_active {
      border-bottom: 1px solid transparent;
      @include media(small) {
        padding: 15px 0;
        line-height: 13px;
      }
      &:after {
        content: "_";
        margin-top: -0.5em;
        margin-right: 0.1em;
        line-height: 0;
        font-size: 1.4em;
      }
    }
  }
  .tab_tag {
    margin-left: 0.5em;
    text-decoration: underline;
  }
  .tab_list {
    +.tab_list {
      margin-top: 15px;
    }
    display: flex;
    .tab_list__icon {
      margin-right: 15px;
      width: 50px;
      height: 50px;
      background-position: center center;
      background-repeat: no-repeat;
      background-size: contain;
      top: 5px;
      position: relative;
      @include media(small) {
        width: 40px;
        height: 40px;
      }
    }
  }
}

//------------------------ POP UP (SIZE GUIDE) ------------------------
.cd-popup {
  position: fixed;
  left: 0;
  top: 0;
  height: 100%;
  width: 100%;
  background-color: rgba(0,0,0, 0.7);
  opacity: 0;
  visibility: hidden;
  -webkit-transition: opacity 0.3s 0s, visibility 0s 0.3s;
  -moz-transition: opacity 0.3s 0s, visibility 0s 0.3s;
  transition: opacity 0.3s 0s, visibility 0s 0.3s;
  z-index: -1;
}
.cd-popup.is-visible {
  opacity: 1;
  visibility: visible;
  -webkit-transition: opacity 0.3s 0s, visibility 0s 0s;
  -moz-transition: opacity 0.3s 0s, visibility 0s 0s;
  transition: opacity 0.3s 0s, visibility 0s 0s;
  z-index: 9;
}

.cd-popup-container {
  position: absolute;
  overflow: hidden;
  top: 50%;
  left: 50%;
  width: 90%;
  max-width: 80%;
  background: #FFF;
  box-shadow: 0 0 20px rgba(0, 0, 0, 0.2);
  -webkit-transform: translate(-50%,0%);
  -moz-transform: translate(-50%,0%);
  -ms-transform: translate(-50%,0%);
  -o-transform: translate(-50%,0%);
  transform: translate(-50%,0%);
  /* Force Hardware Acceleration in WebKit */
  -webkit-backface-visibility: hidden;
  -webkit-transition-property: -webkit-transform;
  -moz-transition-property: -moz-transform;
  transition-property: transform;
  -webkit-transition-duration: 0.3s;
  -moz-transition-duration: 0.3s;
  transition-duration: 0.3s;
  @include media(small) {
    max-width: 90%;
  }
  .popup-content {
    max-height: 85vh;
    overflow-y: scroll;
    padding: 40px;
    #book_showroom {
      height: 60vh!important;
    }
    @include media(small) {
      padding: 20px;
    }
  }
  &.square-popup {
    max-width: 50%!important;
    @include media(small) {
      max-width: 90%!important;
    }
  }
}
.cd-popup-close {
  position: absolute;
  top: 8px;
  right: 8px;
  width: 30px;
  height: 30px;
  text-indent: -99999px;
  @include media(small) {
    top: 0;
    right: 0;
  }
}
.cd-popup-close::before,.cd-popup-close::after {
  content: '';
  position: absolute;
  top: 12px;
  width: 14px;
  height: 3px;
  background-color: #000;
}
.cd-popup-close::before {
  -webkit-transform: rotate(45deg);
  -moz-transform: rotate(45deg);
  -ms-transform: rotate(45deg);
  -o-transform: rotate(45deg);
  transform: rotate(45deg);
  left: 8px;
}
.cd-popup-close::after {
  -webkit-transform: rotate(-45deg);
  -moz-transform: rotate(-45deg);
  -ms-transform: rotate(-45deg);
  -o-transform: rotate(-45deg);
  transform: rotate(-45deg);
  right: 8px;
}
.is-visible .cd-popup-container {
  -webkit-transform: translate(-50%,-50%);
  -moz-transform: translate(-50%,-50%);
  -ms-transform: translate(-50%,-50%);
  -o-transform: translate(-50%,-50%);
  transform: translate(-50%,-50%);
}
