    {%- render 'swym-header-counter' -%}
    <script>
      if (!window.SwymCallbacks) {
        window.SwymCallbacks = [];
      }
      window.SwymCallbacks.push(swymAddToWishlist); /* Init Here */
      if(window.location.href.indexOf("wishlist") == -1){
        window.SwymCallbacks.push(swymRenderButton); 
      }
     
    
      function swymRenderButton(){
        window._swat.fetch( function(products) {
          products.map(function(product) {
            var addToWishlistButton = document.getElementById("product_"+product.empi);
            if(addToWishlistButton !== null){
              addToWishlistButton.classList.add('disabled', 'swym-added');
            } 
          });    
         
        });
      }
      function swymAddToWishlist(swat) {
        attachClickListeners();
      }
    
      function onAddToWishlistClick(e) {
        
        e.preventDefault();
        var productId = e.currentTarget.getAttribute("data-product-id");
        var variantId = e.currentTarget.getAttribute("data-variant-id");
        var sizes = e.currentTarget.getAttribute("data-sizes");
        var compareprice = e.currentTarget.getAttribute("data-compare-price");
        var badge = e.currentTarget.getAttribute("data-badge");
        var iu = e.currentTarget.getAttribute("data-iu");
        var length = e.currentTarget.getAttribute("data-length");
        var colors = e.currentTarget.getAttribute("data-colors");
        var du = e.target.getAttribute("data-product-url");
        
        window._swat.addToWishList(
        {
          "epi": variantId,
          "du": du,
          "empi": productId,
          "iu" : iu,
          "et": 4,
          "cprops": {sizes: sizes, compareprice: compareprice, badge: badge, colors: colors, length: length}
        },
        function(r) {
          var addToWishlistButton = document.getElementById("product_"+r.empi);
            addToWishlistButton.classList.add('disabled', 'swym-added');
        }
    );
      }
    
      function attachClickListeners() {
        var addToWishlists = document.getElementsByClassName("swym-add-to-wishlist-view-product");
        for (var i = 0; i < addToWishlists.length; i++) {
          addToWishlists[i].addEventListener('click', onAddToWishlistClick, false);
          addToWishlists[i].classList.add('swym-icon', 'swym-heart', 'swym-loaded');
        }
      }
     
